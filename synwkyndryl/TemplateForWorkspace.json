						"source": [
							"def getWriteCSV(df, path):\r\n",
							"    df.write.format(\"csv\") \\\r\n",
							"    .mode(\"overwrite\") \\\r\n",
							"    .option(\"header\", \"true\") \\\r\n",
							"    .save(f\"abfss://landing@stakyndryl.dfs.core.windows.net/{path}\")"
						],
						"outputs": [],
						"execution_count": 4
					},
					{
