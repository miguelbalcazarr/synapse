						"source": [
							"def getReadCSV(path):\r\n",
							"    df = spark.read \\\r\n",
							"    .option(\"header\",True) \\\r\n",
							"    .csv(f\"abfss://landing@stakyndryl.dfs.core.windows.net/{path}\")\r\n",
							"    return df"
						],
						"outputs": [],
