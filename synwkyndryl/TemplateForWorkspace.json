								}
							},
							"collapsed": false
						},
						"source": [
							"dfRefined = df \\\r\n",
							"            .withColumn(\"OnlineOrderFlag\", when(col(\"OnlineOrderFlag\") == \"false\", False) \\\r\n",
							"                                        .otherwise(True)) \\\r\n",
							"            .withColumn(\"Comment\", when(col(\"Comment\").isNull(), \"\") \\\r\n",
							"                                        .otherwise(col(\"Comment\")))\r\n",
							"#display(dfRefined)"
						],
